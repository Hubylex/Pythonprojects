def factorial(x):
   s = 1
   while x>0:
        s = s * x
        x -= 1
   return s

def comb(n,r):
    return int(factorial(n)/(factorial(n-r)*factorial(r)))
print('PASCAL\'S TRIANGLE FOR N = 5')
def pascal(n):
    for i in range(n+1):
         for j in range(i+1):
            
            yield comb(i,j)
         print('\n')
for i in pascal(5):
    print(i,end=' ')