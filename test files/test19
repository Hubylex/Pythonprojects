def isValid(s):
    pairs = {
        '{':'}',
        '(':')',
        '[':']'
        }
    if len(s)%2 != 0:
        return False
    for i in s:
        if i in pairs.values():
            continue
        elif pairs[i] not in s:
            return False
        elif s.index(pairs[i]) - s.index(i)%2 != 0:
            continue
        else:
            return False,i
    return True
print(isValid('([)]'))





